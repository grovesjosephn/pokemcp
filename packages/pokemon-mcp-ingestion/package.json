{
  "name": "pokemon-mcp-ingestion",
  "version": "1.0.0",
  "description": "Pokemon data ingestion for MCP server",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pokemon-mcp-ingestion": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch index.ts",
    "start": "tsx index.ts",
    "ingest": "tsx index.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:run": "vitest --run",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "better-sqlite3": "catalog:"
  },
  "devDependencies": {
    "@types/better-sqlite3": "catalog:",
    "@types/node": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "pokemon",
    "mcp",
    "sqlite",
    "api"
  ]
}
